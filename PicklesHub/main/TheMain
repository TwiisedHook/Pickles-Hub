local Window = OrionLib:MakeWindow({Name = "PICKLES HUB v.2", HidePremium = false, SaveConfig = true, ConfigFolder = "v.2"})
local Tab = Window:MakeTab({
	Name = "Player",
	Icon = "rbxassetid://4483345998",
	PremiumOnly = false
})
Tab:AddButton({
	Name = "TP Crate",
	Callback = function()
      		for i,v in pairs(game:GetService("Workspace").LootSpawners:GetDescendants()) do
		if v.Name == "Clicker" then
			game:GetService("Players").LocalPlayer.Character.HumanoidRootPart.CFrame = v.CFrame
		end
	end
  	end    
})
end
