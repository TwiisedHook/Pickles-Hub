local Library = loadstring(game:HttpGet("https://raw.githubusercontent.com/xHeptc/Kavo-UI-Library/main/source.lua"))()
local Window = Library.CreateLib("PICKLES HUB THAI VERSION PREMIUM ROTS ราคา 10 บาท ", "GrapeTheme")
local Tab = Window:NewTab("ROTS!!!")
local Section = Tab:NewSection("Main")
Section:NewButton("Get free GravityCoil and 15k coins"", "Click to use", function()
    wait(0.5)
    workspace.DoShopPurchase:InvokeServer(game:GetService("Players").LocalPlayer.leaderstats.Coins,-15000,"Chest1","GravityCoil")
    end)
